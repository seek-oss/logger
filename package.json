{
  "name": "@seek/logger",
  "version": "10.0.0",
  "private": false,
  "description": "Standardized logging",
  "homepage": "https://github.com/seek-oss/logger#readme",
  "bugs": {
    "url": "https://github.com/seek-oss/logger/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seek-oss/logger.git"
  },
  "license": "MIT",
  "author": {
    "name": "SEEK"
  },
  "sideEffects": false,
  "main": "./lib-commonjs/index.js",
  "module": "./lib-es2015/index.js",
  "types": "./lib-types/index.d.ts",
  "files": [
    "lib*/**/*.d.ts",
    "lib*/**/*.js",
    "lib*/**/*.js.map",
    "lib*/**/*.json"
  ],
  "scripts": {
    "build": "skuba build-package",
    "clean": "rm -fr lib",
    "format": "skuba format",
    "lint": "skuba lint",
    "release": "pnpm build && changeset publish",
    "stage": "changeset version && pnpm format",
    "test": "skuba test",
    "test:ci": "skuba test --coverage",
    "test:watch": "skuba test --watch"
  },
  "dependencies": {
    "dtrim": "^1.11.0",
    "fast-redact": "^3.5.0",
    "pino": "^9.0.0",
    "pino-std-serializers": "^7.0.0",
    "pure-parse": "1.0.0-rc.1"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.2",
    "@changesets/get-github-info": "0.6.0",
    "@types/fast-redact": "3.0.4",
    "@types/split2": "4.2.3",
    "skuba": "11.1.0",
    "split2": "4.2.0"
  },
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": ">=20.9.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "skuba": {
    "entryPoint": "src/index.ts",
    "template": null,
    "type": "package",
    "version": "11.0.0"
  }
}
